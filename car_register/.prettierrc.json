{
	"trailingComma": "none",
	"tabWidth": 2,
	"semi": true,
	"singleQuote": true,
	"printWidth": 120,
	"useTabs": false,
	"quoteProps": "as-needed",
	"jsxSingleQuote": true,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"arrowParens": "always",
	"singleAttributePerLine": false,
	"endOfLine": "lf"
}
